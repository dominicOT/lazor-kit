{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ESNext",
      "jsx": "react-jsx",
      "moduleResolution": "Node",
      "esModuleInterop": true,
      "noImplicitAny": true,
      "resolveJsonModule": true,
      "strictNullChecks": true,
      "skipLibCheck": true
    },
    "include": ["src"]
}